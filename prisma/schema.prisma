generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model MarketItem {
  id            Int    @id @unique
  name          String
  current_stock BigInt
  total_trades  BigInt
  base_price    BigInt
  main_category Int
  sub_category  Int

  created_at DateTime @default(now())
  updated_at DateTime @updatedAt

  @@map("market_items")
}
